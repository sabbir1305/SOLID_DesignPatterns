@model Dictionary<SRPAndDesignPatterns_V1.Models.Interface.IReadableSettings, Dictionary<string, string>>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>All Settings</h1>
<form asp-controller="Settings" asp-action="Save" method="post">
    <input type="submit" value="Change Settings" />
</form>

<table border="1" cellpadding="10">
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Key.ToString()</td>
            <td>
                @foreach (var subItem in item.Value)
                {
                    <div>@subItem.Key = @subItem.Value</div>
                }
            </td>
        </tr>
    }
</table>

